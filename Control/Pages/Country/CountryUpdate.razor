@page "/Country/Update/{CountryId}"





<AuthorizeView Roles="Administrador,Editor">
    <Authorized>    <Animate Animation="Animations.FadeRight" Duration="TimeSpan.FromSeconds(0.1)" Delay="TimeSpan.FromSeconds(0)">
        <div>
            <h2>Actualización de País</h2>
            <UpdateCountryComponent id="@CountryId.ToString()" />
        </div>
    </Animate></Authorized>
    <NotAuthorized>
        <NotAvariableComponent/>
    </NotAuthorized>
</AuthorizeView>






@code {
    [Parameter]
    public string CountryId { get; set; }


    protected override async Task OnInitializedAsync()
    {
        CountryId = CountryId;
    }
} 