@page "/device/view/{DeviceId}"

<div>
    <h2 class="text-center">Información del dispositivo</h2>
    <hr />
    <a href="device/list"> Regresar a la lista de dispositivos</a>
    <hr />
    <DeviceViewComponent DeviceId="@DeviceId"/>
           
</div>


@code{
    [Parameter]
    public string DeviceId { get; set; }
    protected override async Task OnInitializedAsync()
    {
        DeviceId = DeviceId;
    }
}


