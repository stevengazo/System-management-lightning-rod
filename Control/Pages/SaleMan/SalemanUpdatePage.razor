@page "/saleman/update/{SaleManId}"


<div>
    <h2 class="text-center">Actualizar Vendedor</h2>
    <hr />
        <div class="d-flex flex-row justify-content-between">
        <p>
            Modifique la información del vendedor.
        </p>
        <a href="saleman/list" class="btn btn-sm btn-info "> Lista de vendedores</a>
    </div>
    <hr />
    <SaleManUpdateComponet id="@SaleManId.ToString()"/>
</div>



@code{
    [Parameter]
    public string SaleManId { get; set; }
    protected override async Task OnInitializedAsync()
    {
        SaleManId = SaleManId;
    }
}
