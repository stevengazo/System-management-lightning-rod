@page  "/incident/update/{IncidentId}"

<div>
    <h2 class="text-center">Actualizar Incidente</h2>
    <hr />
    <a href="incident/list"> Regresar a la lista de incidentes</a>
    <hr />
    <IncidentUpdateComponent IncidentId="@IncidentId"/>
</div>



@code{
    [Parameter]
    public string IncidentId { get; set; }

    protected override async Task OnInitializedAsync()
    {
        IncidentId = IncidentId;
    }
}
