 @page "/manage/EditUser/{UserId}"


<AuthorizeView Roles="Administrador">
    <Authorized>
        <h1>Información del Usuario</h1>
        <EditUserComponent UserId="@UserId.ToString()"/>         
    </Authorized>
    <NotAuthorized>
        <h2 class="text-danger">No posee autorización para ver esta pagina</h2>
    </NotAuthorized>
</AuthorizeView>



@code{
    [Parameter]
    public string UserId { get; set; }

    protected override async Task OnInitializedAsync()
    {
    UserId = UserId;
    } 
}

    